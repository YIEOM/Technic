<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yieom.javastudy.technic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Technic"
        android:name=".App">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="yieom.javastudy.contentprovidera.MainActivity"
            android:taskAffinity="yieom.javastudy.contentprovider">
        </activity>
        <activity android:name="yieom.javastudy.contentproviderb.MainActivity"
            android:taskAffinity="yieom.javastudy.contentprovider">
        </activity>
    </application>

</manifest>